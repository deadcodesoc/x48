#include "../config.h"

#ifdef CC_DEF
             CC = CC_DEF
#endif

#ifdef CDEBUGFLAGS_DEF
    CDEBUGFLAGS = CDEBUGFLAGS_DEF
#endif

       DEFINES = -DANSI_ARROWS -DHAVE_TCGETATTR -DUSE_DIRENT -DSYS_UNIX
          SRCS = complete.c editline.c sysunix.c
          OBJS = complete.o editline.o sysunix.o
      TARFILES = Imakefile README editline.h unix.h $(SRCS)

NormalLibraryTarget(readline, $(OBJS))
DependTarget()

echo_tarfiles:
	@echo $(TARFILES)
